{
  "name": "ui",
  "version": "1.0.0",
  "description": "UI package for Voluspa projects",
  "keywords": [
    "ui",
    "typescript"
  ],
  "homepage": "https://github.com/voluspa-labs/ui",
  "bugs": {
    "url": "https://github.com/voluspa-labs/ui/issues"
  },
  "author": "Voluspa <info@volsupa.io> (https://volsupa.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voluspa-labs/ui.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vitest",
    "build": "tsc",
    "ci": "bun run build && bun run check-format && bun run check-exports && bun run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  },
  "packageManager": "bun@1.1.34",
  "dependencies": {
    "@changesets/cli": "^2.27.9"
  }
}
